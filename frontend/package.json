    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.0"
  }
}
